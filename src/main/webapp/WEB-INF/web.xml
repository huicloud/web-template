<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
      version="3.0">
      
	<servlet>
		<servlet-name>RdFileServlet</servlet-name>
		<servlet-class>cn.com.gw.servlet.RdFileServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RdFileServlet</servlet-name>
		<url-pattern>/RdFileServlet</url-pattern>
	</servlet-mapping>
	
	<listener>
		<listener-class>cn.com.gw.listener.TokenListener</listener-class>
	</listener>
	
	<error-page>
		<error-code>404</error-code>
		<location>/NotFound404.jsp</location>
	</error-page>
	
	<welcome-file-list>
		<welcome-file>/dzh/index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
